{
    "name": "Blur Page",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Extension blur page onBlur window",
    "content_scripts": [
        {
            "matches": [
                "http://www.messenger.com/*",
                "https://www.messenger.com/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "browser_action": {
        "default_icon": "icons/blur-on.png"
    }
}